{
  "name": "vue-lunata",
  "version": "0.1.0",
  "description": "Data-driven tables for Vue",
  "repository": "https://github.com/bg-wilkesreid/vue-lunata",
  "author": "bg-wilkesreid <samr@bureaugravity.com>",
  "license": "MIT",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build-lib": "vue-cli-service build --target lib --name vue-lunata ./src/index.js && cp dist/vue-lunata.umd.js docs/ && cp dist/vue-lunata.common.js docs/"
  },
  "dependencies": {
    "core-js": "^2.6.5",
    "node-sass": "^4.12.0",
    "vue": "^2.6.10",
    "vue-context": "^4.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.7.0",
    "@vue/cli-plugin-eslint": "^3.7.0",
    "@vue/cli-service": "^3.7.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
